{
  "name": "style-suggestions-addin",
  "version": "1.0.0",
  "description": "Word add-in for applying style suggestions to documents",
  "main": "src/taskpane/taskpane.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "start": "office-addin-debugging start manifest.xml",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "sideload": "office-addin-dev-settings sideload manifest.xml",
    "unload": "office-addin-dev-settings remove manifest.xml"
  },
  "keywords": [
    "Office Add-in",
    "Word",
    "Style",
    "Formatting"
  ],
  "author": "Style Verification Service",
  "license": "MIT",
  "dependencies": {
    "@microsoft/office-js": "^1.1.85",
    "@microsoft/office-js-helpers": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "office-addin-debugging": "^5.0.0",
    "office-addin-dev-certs": "^1.11.3",
    "office-addin-lint": "^2.2.5",
    "office-addin-manifest": "^1.12.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browserslist": [
    "ie 11"
  ]
} 